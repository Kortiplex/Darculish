			"name": "Annotation",
			"scope": [
				"storage.type.annotation",
				"punctuation.definition.annotation",
				"meta.decorator",
				"meta.decorator entity.name.function",
				"punctuation.decorator"
			],
			"settings": {
				"foreground": "#BBB529",
				"fontStyle": ""
			}
		},
		{
			"name": "Decorator Object Key",
			"scope": [
				"meta.decorator meta.object-literal.key",
			],
			"settings": {
				"foreground": "#9876aa",
				"fontStyle": ""
			}
		},
		{
			"name": "Annotation Extras",
			"scope": [
				"punctuation.definition.annotation-arguments.begin.bracket.round.java",
				"punctuation.definition.annotation-arguments.end.bracket.round.java",
				"meta.decorator meta.brace",
				"meta.decorator variable"
			],
			"settings": {
				"foreground": "#A9B7C6",
				"fontStyle": ""
			}
		},


		// -> Classes/Types/Interfaces
		{
			"name": "Class/Type/Interface Entity Identifier",
			"scope": [
				"entity.name.type.class",
				"entity.name.type.interface"
