	"tokenColors": [
		{
			"scope": "emphasis",
			"settings": {
				"fontStyle": "italic"
			}
		},
		{
			"scope": "strong",
			"settings": {
				"fontStyle": "bold"
			}
		},
		{
			"scope": "header",
			"settings": {
				"foreground": "#000080"
			}
		},

		// -> Start Comment Section
		{
			"name": "Comment",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#546E7A"
			}
		},

		{
			"scope": ["comment.line", "comment.block"],
			"settings": {
				"fontStyle": "",
				"foreground": "#808080"
			}
		},
			{
			"scope": [
				"comment.block.preprocessor",
				"comment.block.documentation",
				"comment.block.documentation entity.name.type.instance",
				"comment.block.documentation variable.other",
				"comment.block.documentation keyword.other.type",
				"comment.block.documentation support.class",
				"comment.block.documentation punctuation.separator.delimiter",
				"string.quoted.docstring.multi.python"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#808080"
			}
		},
		{
			"scope": [
				"comment.line keyword.codetag.notation",
				"comment.block.documentation keyword.other",
				"comment.block.documentation storage.type.class"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#77B767"
			}
			},
			{
			"scope": ["comment.line.scss", "comment.block.scss"],
			"settings": {
				"fontStyle": "",
				"foreground": "#BC9455"
			}
		},
		// -> End Comment Section

