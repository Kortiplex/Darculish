			"name": "Class/Type/Interface Variable",
			"scope": [
				"meta.definition.variable storage.type",
				"meta.method.identifier storage.type",
				// ts class variable name
				"entity.name.type",
				"support.class",
				// New object declaration
				"new.expr entity.name.function",
			],
			"settings": {
				"foreground": "#d9e4eb",
				"fontStyle": ""
			}
		},
		{
			"name": "Class/Type Variable Enclosure",
			"scope": [
