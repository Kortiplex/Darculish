				"meta.method.return-type storage.type"
			],
			"settings": {
				"foreground": "#d9e4eb",
				"fontStyle": ""
			}
		},

		{
			"name": "Inherited Classes/Interfaces",
			"scope": [
				"entity.other.inherited-class",
			],
			"settings": {
				"foreground": "#d9e4eb",
				"fontStyle": ""
			}
		},


